  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch development",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_development.dart",
      "deviceId": "chrome",
      "args": [
        "--flavor",
        "development",
        "--target",
        "lib/main_development.dart"
      ]
    },
    {
      "name": "(WEB) Launch development",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_development.dart",
      "deviceId": "chrome",
      "args": [
        "--flavor",
        "development",
        "--target",
        "lib/main_development.dart",
        "--dart-define",
        "GOOGLE_MAPS_API_KEY=YOUR_API_KEY",
        "--web-browser-flag",
        "--disable-web-security"
      ]
    },
    {
      "name": "Launch staging",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_staging.dart",
      "args": ["--flavor", "staging", "--target", "lib/main_staging.dart",
    "--dart-define",
                "GOOGLE_MAPS_API_KEY=AIzaSyB1gWtaVetGLyySosC92hOMSq2razu-01E"
    ]
    },
    {
      "name": "Launch production",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_production.dart",
      "args": ["--flavor", "production", "--target", "lib/main_production.dart",
    "--dart-define",
                "GOOGLE_MAPS_API_KEY=AIzaSyB1gWtaVetGLyySosC92hOMSq2razu-01E"
              ]
    }
  ]
}
